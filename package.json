{
  "name": "my-blog",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "generate": "node scripts/gen-posts-index.js",
    "tsc": "tsc",
    "docs:dev": "vitepress dev docs --config docs/.vitepress/config.js",
    "generate-posts-index": "node scripts/gen-posts-index.js",
    "docs:build": "npm run generate && vitepress build docs --config docs/.vitepress/config.js",
