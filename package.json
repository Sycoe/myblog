{
  "name": "my-blog",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "tsc": "tsc",
    "docs:dev": "vitepress dev docs --config docs/.vitepress/config.js",
    "docs:build": "vitepress build docs --config docs/.vitepress/config.js",
    "docs:serve": "vitepress serve docs --config docs/.vitepress/config.js",
    "deploy": "npm run docs:build && gh-pages -d docs/.vitepress/dist"
  },
  "devDependencies": {
    "@types/node": "^24.0.13",
    "gh-pages": "^6.3.0",
    "typescript": "^5.8.3",
    "vitepress": "^1.6.3"
  },
  "dependencies": {
    "@giscus/vue": "^3.1.1"
  }
}
